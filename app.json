{
  "name": "String Session Generator",
  "description": "Telegram bot to Generate Pyrogram & Telethon StringSession Made By Dino Guardian (@DinoGuardian)",
  "logo": "https://telegra.ph/file/f1066d3293910d5cd3307.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/GUARDIANS-BOTS/STRINGBOT",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get This Value From my.telegram.org or @Api_ScrapperBot.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get This Value from my.telegram.org or @Api_ScrapperBot.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Get The BotToken From @BotFather ",
      "required": true,
      "value": ""
    }
  }
}
